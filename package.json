{
  "name": "vue-rest-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-prod": "vue-cli-service build --mode prod",
    "build-0": "vue-cli-service build --target wc-async --name comp1  src/components/*.vue",
    "build-1": "vue-cli-service build --target wc-async --name vue-rest-api-views src/views/*.vue",
    "build-2": "vue-cli-service build --target wc-async --name vue-rest-api-Dashboard src/views/Dashboard/*.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^2.6.5",
    "css-loader": "^2.1.1",
    "laydate": "^1.0.7",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.6",
    "vue-cookies": "^1.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  }
}
